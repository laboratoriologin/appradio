        <Grid HorizontalAlignment="Left" Width="900">
            <StackPanel VerticalAlignment="Bottom" Background="Transparent" RenderTransformOrigin="0.506,1.738" HorizontalAlignment="Right" Width="900">
                <Grid Width="900">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="156"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <StackPanel Width="156" Background="#E5EA2227">
                        <TextBlock x:Name="DataHorarioEvento"
								   TextAlignment="Center" 
								   TextWrapping="Wrap" 
								   Text="{Binding DataHorarioEvento}" 
								   Margin="0,10,0,10" 
								   FontFamily="Segoe UI" 
								   FontSize="32" 
								   HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </StackPanel>
                    <StackPanel Grid.Column="1"  Background="#E5DCDCDC"  Width="Auto">
                        <Grid Width="Auto">
